{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "trailingComma": "none",
  "bracketSameLine": true,
  "endOfLine": "auto",
  "printWidth": 120,
  "linebreak-style": [true, "LF"],
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "html"
      }
    }
  ]
}
